{
  "name": "sqs-consumer-starter",
  "version": "1.0.0",
  "description": "Examples and automated tests for sqs-consumer and sqs-producer",
  "private": true,
  "engines": {
    "node": ">=22.x"
  },
  "scripts": {
    "test": "npm run test --workspaces --if-present",
    "test:system": "npm run test --workspace @sqs-consumer-starter/system-tests",
